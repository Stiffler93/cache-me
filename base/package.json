{
    "name": "@cache-me/base",
    "version": "1.0.1",
    "description": "cache-me removes the overhead of writing explicit logic just for caching. Instead of having to deal with imperative calls to store and load data from a cache, it allows you to use your plain old functions just the way you're used to.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "commonjs",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "eslint -c ../.eslintrc.json --ext .ts .",
        "format": "prettier --ignore-path ../.prettierignore --write \"**/*.+(ts|json)\"",
        "test": "jest"
    },
    "keywords": [
        "promises",
        "cache",
        "functions"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Stiffler93/cache-me.git"
    },
    "author": "Stefan Sonntag <sonntag19@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.8.0",
        "@types/object-hash": "^3.0.4",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "object-hash": "^3.0.0"
    }
}
