{
    "name": "@cache-me/redis",
    "version": "1.0.0",
    "description": "An add-on to @cach-me/base that adds support for Redis.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "commonjs",
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "tsc",
        "lint": "eslint -c ../.eslintrc.json --ext .ts .",
        "format": "prettier --ignore-path ../.prettierignore --write \"**/*.+(ts|json)\"",
        "test": "jest --detectOpenHandles"
    },
    "keywords": [
        "promises",
        "cache",
        "functions",
        "redis",
        "@cache-me"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Stiffler93/cache-me.git"
    },
    "author": "Stefan Sonntag <sonntag19@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.8.0",
        "ioredis-mock": "^8.9.0",
        "jest": "^29.7.0",
        "redis-memory-server": "^0.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "ioredis": "^5.3.2"
    }
}
