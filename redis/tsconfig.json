{
  "compilerOptions": {
    "baseUrl": "./",
    "isolatedModules": false,
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "strict": true,
    "target": "es2020",
    "paths": {
      "src/*": [
        "./src/*"
      ]
    }
  }
}
